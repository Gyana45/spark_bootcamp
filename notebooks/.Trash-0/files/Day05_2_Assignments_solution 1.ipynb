   "source": [
    "df_orders.where(lower(col('order_status'))!='cancelled')\\\n",
    "            .groupBy(col('state'),year(col('order_date')).alias('year'))\\\n",
    "                  .agg(sum('quantity').alias('total_quantity'),sum(col('price')*col('quantity')).alias('revenue'))\\\n",
    "                  .filter(col('revenue')>550000)\\\n",
    "                   .sort(col('revenue').desc())\\\n",
    "                      .show()\n"
   ]
  },
